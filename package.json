{
  "name": "craft-and-local-client-v2",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Gethyn Jones",
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "yarn": "1.6.0"
  },
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "NODE_ENV=development webpack-dev-server --progress",
    "start:json": "./node_modules/.bin/json-server --watch ./src/data/db.json",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress && cp _redirects dist/",
    "lint": "eslint src --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "yarn test",
    "prepush": "yarn test"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "common/components(.*)$": "<rootDir>/src/app/common/components/$1"
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-flow-react-proptypes": "^24.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.71.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-fetch-mock": "^1.5.0",
    "normalize.css": "^8.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-async-script-loader": "^0.3.0",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-mq": "^4.0.2",
    "whatwg-fetch": "^2.0.4",
    "yarn": "^1.6.0"
  }
}
